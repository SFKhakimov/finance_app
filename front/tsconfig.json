{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "common/*": ["common/*"],
      "lk/*": ["modules/lk/*"],
      "store/*": ["modules/store/*"]
    },
  }
}
